<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.unal.tictactoe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".multiplayer.GameListActivity"></activity>
        <activity android:name=".multiplayer.NewGameDialogFragment" />
        <activity android:name=".multiplayer.MenuMulPActivity" />
        <activity android:name=".single.AndroidTicTacToeActivity" />
        <activity android:name=".multiplayer.TicTacToeMulActivity" />
        <activity android:name=".general.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Settings"
            android:label="Settings" />
    </application>

</manifest>